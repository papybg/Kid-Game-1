{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm install --include=dev && esbuild server/index.ts --platform=node",
    "start": "NODE_ENV=production node dist/index.js",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx seed.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@types/sharp": "^0.31.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "multer": "^2.0.2",
    "postgres": "^3.4.7",
    "sharp": "^0.34.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "20.16.11",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
